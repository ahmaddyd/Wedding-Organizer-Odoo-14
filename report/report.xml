<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_wedding_order">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <div style="margin-top:150px">
                        <t t-call="weddingorganizer.report_wo_order"/>
                    </div>
                </t>
            </t>
        </template>

        <record id="action_report_wo_order" model="ir.actions.report">
            <field name="name">Report Order Wedding Organizer</field>
            <field name="model">wedding.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">weddingorganizer.report_wedding_order</field>
            <field name="report_file">weddingorganizer.report_wedding_order</field>
            <field name="binding_model_id" ref="model_wedding_order"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>